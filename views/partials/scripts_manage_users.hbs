<script>
    async function onclickDelete(id) {
        await $.ajax({
            url: `/account/${id}`,
            type: 'PUT',
            data: {
                is_deleted: true
            },
            success: async function (response, status) {
                const res = await response;
                window.location.href = '/manage-users';
            }
        });
    }

</script>
